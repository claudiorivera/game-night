{
	"name": "game-night",
	"version": "0.3.0",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@10.5.2",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"vercel-build": "pnpm db:migrate && next build",
		"start": "next start",
		"check:fix": "pnpm biome check --write src",
		"check:nofix": "pnpm biome check src",
		"check-types": "tsc --noEmit",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:seed": "pnpm with-env tsx src/server/db/runSeed.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"knip": "SKIP_ENV_VALIDATION=true knip",
		"with-env": "dotenvx run --"
	},
	"dependencies": {
		"@auth/drizzle-adapter": "1.7.4",
		"@faker-js/faker": "9.5.0",
		"@libsql/client": "0.14.0",
		"@paralleldrive/cuid2": "2.2.2",
		"@radix-ui/react-avatar": "1.1.3",
		"@radix-ui/react-dialog": "1.1.6",
		"@radix-ui/react-dropdown-menu": "2.1.6",
		"@radix-ui/react-label": "2.1.2",
		"@radix-ui/react-popover": "1.1.6",
		"@radix-ui/react-slot": "1.1.2",
		"@radix-ui/react-tooltip": "1.1.8",
		"@sentry/nextjs": "9.2.0",
		"@t3-oss/env-nextjs": "0.12.0",
		"@tanstack/react-query": "5.66.9",
		"bgg-sdk": "1.0.0",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"cmdk": "1.0.4",
		"date-fns": "4.1.0",
		"drizzle-orm": "0.40.0",
		"lucide-react": "0.476.0",
		"next": "15.2.0",
		"next-auth": "5.0.0-beta.25",
		"postgres": "3.4.5",
		"react": "19.0.0",
		"react-day-picker": "8.10.1",
		"react-dom": "19.0.0",
		"react-hot-toast": "2.5.2",
		"tailwind-merge": "3.0.2",
		"tailwindcss-animate": "1.0.7",
		"usehooks-ts": "3.1.1",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@dotenvx/dotenvx": "1.38.3",
		"@tailwindcss/postcss": "^4.0.9",
		"@types/node": "22.13.5",
		"@types/react": "19.0.10",
		"@types/react-dom": "19.0.4",
		"drizzle-kit": "0.30.5",
		"knip": "5.45.0",
		"postcss": "8.5.3",
		"tailwindcss": "4.0.9",
		"tsx": "4.19.3",
		"typescript": "5.7.3"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome",
			"@sentry/cli",
			"esbuild",
			"sharp"
		]
	}
}
