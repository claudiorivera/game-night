{
	"name": "game-night",
	"version": "0.3.0",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@9.15.4",
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"vercel-build": "pnpm db:migrate && next build",
		"start": "next start",
		"check:fix": "pnpm biome check --write src",
		"check:nofix": "pnpm biome check src",
		"check-types": "tsc --noEmit",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:seed": "pnpm with-env tsx src/server/db/runSeed.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"knip": "SKIP_ENV_VALIDATION=true knip",
		"with-env": "dotenvx run --"
	},
	"dependencies": {
		"@auth/drizzle-adapter": "1.7.4",
		"@faker-js/faker": "9.4.0",
		"@libsql/client": "0.14.0",
		"@paralleldrive/cuid2": "2.2.2",
		"@radix-ui/react-avatar": "1.1.2",
		"@radix-ui/react-dialog": "1.1.5",
		"@radix-ui/react-dropdown-menu": "2.1.5",
		"@radix-ui/react-label": "2.1.1",
		"@radix-ui/react-popover": "1.1.5",
		"@radix-ui/react-slot": "1.1.1",
		"@radix-ui/react-tooltip": "1.1.7",
		"@sentry/nextjs": "8.52.1",
		"@t3-oss/env-nextjs": "0.12.0",
		"@tanstack/react-query": "5.65.1",
		"bgg-sdk": "1.0.0",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"cmdk": "1.0.4",
		"date-fns": "4.1.0",
		"drizzle-orm": "0.39.1",
		"lucide-react": "0.474.0",
		"next": "15.1.6",
		"next-auth": "5.0.0-beta.25",
		"postgres": "3.4.5",
		"react": "19.0.0",
		"react-day-picker": "8.10.1",
		"react-dom": "19.0.0",
		"react-hot-toast": "2.5.1",
